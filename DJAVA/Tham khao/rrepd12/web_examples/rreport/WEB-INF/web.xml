<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

    <display-name>RReport Examples</display-name>
    <description>
      RReport Examples.
    </description>





    <!-- Define servlets that are included in the example application -->

    <servlet>
        <servlet-name>RReportServlet</servlet-name>
        <servlet-class>RReportServlet</servlet-class>
    </servlet>
    

    <servlet-mapping>
        <servlet-name>RReportServlet</servlet-name>
        <url-pattern>/servlet/RReportServlet</url-pattern>
    </servlet-mapping>



</web-app>
